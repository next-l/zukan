<h1>Listing type_of_names</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Display name</th>
    <th>Note</th>
    <th>Position</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @type_of_names.each do |type_of_name| %>
  <tr>
    <td><%= type_of_name.name %></td>
    <td><%= type_of_name.display_name %></td>
    <td><%= type_of_name.note %></td>
    <td><%= type_of_name.position %></td>
    <td><%= link_to 'Show', type_of_name %></td>
    <td><%= link_to 'Edit', edit_type_of_name_path(type_of_name) %></td>
    <td><%= link_to 'Destroy', type_of_name, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

